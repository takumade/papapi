{
  "name": "papapi",
  "description": "Is a payment microservice for Paynow, Paypal and Stripe",
  "version": "0.1.1",
  "homepage": "",
  "private": true,
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Takunda Madechangu",
    "email": "madechangu.takunda@gmail.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "^14.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "test": "npm run lint && npm run compile && npm run mocha",
    "lint": "eslint src/. test/. --config .eslintrc.json --ext .ts --fix",
    "dev": "ts-node-dev src/",
    "start": "npm run compile && node lib/",
    "mocha": "mocha --require ts-node/register --require source-map-support/register \"test/**/*.ts\" --recursive --exit",
    "compile": "shx rm -rf lib/ && tsc"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "ignore": []
  },
  "types": "lib/",
  "dependencies": {
    "@feathersjs/authentication": "^4.5.15",
    "@feathersjs/authentication-local": "^4.5.15",
    "@feathersjs/authentication-oauth": "^4.5.15",
    "@feathersjs/configuration": "^4.5.15",
    "@feathersjs/errors": "^4.5.15",
    "@feathersjs/express": "^4.5.15",
    "@feathersjs/feathers": "^4.5.15",
    "@feathersjs/socketio": "^4.5.15",
    "@feathersjs/transport-commons": "^4.5.15",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "feathers-sequelize": "^6.3.4",
    "helmet": "^5.1.0",
    "mysql2": "^2.3.3",
    "paynow": "git+https://github.com/iamngoni/Paynow-NodeJS-SDK.git",
    "paypal-rest-sdk": "^1.8.1",
    "sequelize": "^6.21.2",
    "serve-favicon": "^2.5.0",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@swc/core": "^1.2.215",
    "@swc/helpers": "^0.4.3",
    "@types/bluebird": "^3.5.36",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mocha": "^9.1.1",
    "@types/serve-favicon": "^2.5.3",
    "@types/validator": "^13.7.4",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "axios": "^0.27.2",
    "eslint": "^8.19.0",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.19",
    "regenerator-runtime": "^0.13.9",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  }
